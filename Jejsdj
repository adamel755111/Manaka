print("Dupe working")

-- Maak notificatie aan
local gui = Instance.new("ScreenGui")
gui.Name = "DupeNotif"
gui.ResetOnSpawn = false
gui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

local label = Instance.new("TextLabel")
label.Size = UDim2.new(0, 200, 0, 50)
label.Position = UDim2.new(0.5, -100, 0.9, 0)
label.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
label.BackgroundTransparency = 0.3
label.TextColor3 = Color3.fromRGB(0, 255, 0)
label.TextStrokeTransparency = 0.6
label.Font = Enum.Font.SourceSansBold
label.TextSize = 24
label.Text = "Dupe working"
label.Parent = gui

-- Delay en uitvoeren van grab
task.delay(1, function()
    local rStorage = game:GetService("ReplicatedStorage")
    local netPackage = rStorage:WaitForChild("Packages"):WaitForChild("Net")
    local grabEvent = netPackage:FindFirstChild("RE/StealService/Grab")

    if not grabEvent then
        warn("Grab event not found.")
        return
    end

    local function sendGrab(id)
        grabEvent:FireServer("Grab", id)
    end

    local Grabber = {}
    Grabber.__index = Grabber

    function Grabber.new(id)
        local self = setmetatable({}, Grabber)
        self.id = id
        print(">> [Client] Grab Queued:", id)
        return self
    end

    function Grabber:execute()
        print(">> [Client] Executing Grab:", self.id)
        sendGrab(self.id)
    end

    local generatedID = math.floor(os.clock() * 100000)
    local grabInstance = Grabber.new(generatedID)
    task.wait(0.5)
    grabInstance:execute()

    gui:Destroy()
end)
